					}
				},
				"fileLocation": [
					"relative",
					"${workspaceFolder}"
				],
				"name": "dapr",
				"pattern": [
					{
						"file": 1,
						"location": 2,
						"message": 3,
						"regexp": "."
					}
				]
			}
		],
